<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.blusoft.nmreader">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/custom_ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BrowseMangaActivity" />
        <activity android:name=".Brownse_by.ShowListsActivity" />
        <activity android:name=".Brownse_by.BrowseByActivity" />
    </application>

</manifest>